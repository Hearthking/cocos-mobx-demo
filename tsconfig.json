{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    ".vscode",
    "library",
    "local",
    "settings",
    "temp",
    "build",
    "build-template"
  ]
}